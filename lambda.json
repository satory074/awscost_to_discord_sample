{
    "name": "awscost_to_discord",
    "description": "function to notify discord of AWS cost",
    "runtime": "python3.9",
    "region": "ap-northeast-1",
    "handler": "lambda_function.lambda_handler",
    "role": "[role]",
    "timeout": 300,
    "memory": 128,
    "variables":
      {
        "WebhookURL": "[WebhookURL]",
        "budgetName":"[budgetName]",
        "accountId":"[accountId]"
      }
  }